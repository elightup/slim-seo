		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"babel-loader": "^8.1.0",
		"sass": "^1.51.0",
		"webpack": "^5.73.0",
		"webpack-cli": "^3.3.12"
	},
	"babel": {
